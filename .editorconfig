# 表示是最顶层的 EditorConfig 配置文件
root = true

[*] # 表示所有文件适用 [*.{js,jsx,ts,tsx,vue}]
charset = utf-8 # 设置文件字符集为 utf-8
indent_style = space
indent_size = 2
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 100

[*.md] # 表示仅 md 文件适用以下规则
max_line_length = off
trim_trailing_whitespace = false
